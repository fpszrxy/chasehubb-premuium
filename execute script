local URLS = {
    ['bladeball'] = 'https://raw.githubusercontent.com/fpszrxy/chasehubb-premuium/refs/heads/main/Bladeball',
    ['plants vs brainrots'] = 'https://raw.githubusercontent.com/fpszrxy/chasehubb-premuium/refs/heads/main/pvb'
}

local GAME_IDS = {
    [13772394625] = 'bladeball',
    [14525514348] = 'bladeball',
    [14732610803] = 'bladeball',
    [127742093697776] = 'plants vs brainrots'
}

local currentPlaceId = game.PlaceId
local gameName = GAME_IDS[currentPlaceId]

if gameName and URLS[gameName] then
    local scriptUrl = URLS[gameName]
    loadstring(game:HttpGet(scriptUrl))()
    print('Loaded script for ' .. gameName)
else
    warn('No script found for PlaceId: ' .. tostring(currentPlaceId))
end
